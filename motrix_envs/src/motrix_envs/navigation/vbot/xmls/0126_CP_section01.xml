<mujoco>
  <compiler angle="radian" assetdir="assets" autolimits="true"/>

  <asset>

    <!-- Textures -->

    <!-- Materials -->
    <material name="Material" rgba="0.800000011920929 0.800000011920929 0.800000011920929 0.3" reflectance="0.5" metallic="0.0" roughness="0.5"/>

    <!-- Visual meshes -->
    <mesh class="visual" name="V_cpin" file="V_cpin.obj"/>
    <mesh class="visual" name="V_cpout" file="V_cpout.obj"/>

    <!-- Collision meshes -->
  </asset>

  <default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2" density="0"/>
      </default>
      <default class="collision">
        <geom type="mesh" rgba="1 1 1 1" group="3" contype="1" conaffinity="1"/>
      </default>
  </default>

  <worldbody>

    <body name="B_cp_body" pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0">
      <geom class="visual" mesh="V_cpin" pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" material="Material"/>
      <geom class="visual" mesh="V_cpout" pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" material="Material"/>
    </body>
  </worldbody>

</mujoco>
