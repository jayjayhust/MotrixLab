    <texture type="2d" colorspace="linear" name="BAKE_INSIDE_Bake1_CyclesBake_COMBINED_ro" file="BAKE_INSIDE_Bake1_CyclesBake_COMBINED_ro.png"/>
    <texture type="2d" colorspace="sRGB" name="BAKE_OUTSIDE_A_Bake1_CyclesBake_COMBINED" file="BAKE_OUTSIDE_A_Bake1_CyclesBake_COMBINED.png"/>
    <texture type="2d" colorspace="sRGB" name="BAKE_OUTSIDE_B_Bake1_CyclesBake_COMBINED" file="BAKE_OUTSIDE_B_Bake1_CyclesBake_COMBINED.png"/>
    <texture type="2d" colorspace="sRGB" name="ENDPOINT_Bake1_CyclesBake_COMBINED" file="ENDPOINT_Bake1_CyclesBake_COMBINED.png"/>
    <texture type="2d" colorspace="sRGB" name="LANTERN_BODY_BK_Bake1_CyclesBake_COMBINED" file="LANTERN_BODY_BK_Bake1_CyclesBake_COMBINED.png"/>
