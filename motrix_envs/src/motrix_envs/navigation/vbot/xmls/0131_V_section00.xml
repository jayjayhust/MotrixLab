    <material name="MI_BAKE_OUTSIDE_B" rgba="1 1 1 1" reflectance="0.5" metallic="0.0" roughness="1.0">
      <layer texture="BAKE_OUTSIDE_B_Bake1_CyclesBake_COMBINED" role="rgb"/>
    </material>
    <material name="MI_ENDPOINT" rgba="1 1 1 1" reflectance="0.5" metallic="0.0" roughness="0.20000000298023224">
      <layer texture="ENDPOINT_Bake1_CyclesBake_COMBINED" role="rgb"/>
    </material>
    <material name="bluelight.001"
             rgba="0.0 0.0 1.0 1.0"
             emission="1 1 1 1.0"
             emissionintensity="10.0"/>
    <material name="bluelight"
             rgba="0.0 0.0 1.0 1.0"
             emission="1 1 1 1.0"
             emissionintensity="10.0"/>
    <material name="MI_LANTERN_BODY_BK" rgba="1 1 1 1" emission="0.5 0.5 0.5 1.0" emissionintensity="0.2">
      <layer texture="LANTERN_BODY_BK_Bake1_CyclesBake_COMBINED" role="rgb"/>
      <layer texture="LANTERN_EMMI" role="emissive"/>
    </material>
    <material name="MI_LANTERN_CLOUR_BK" rgba="1 1 1 1" emission="0.5 0.5 0.5 1.0" emissionintensity="0.2">
      <layer texture="LANTERN_CLOUR_BK_Bake1_CyclesBake_COMBINED" role="rgb"/>
      <layer texture="LANTERN_CLOUD_EMMI" role="emissive"/>
    </material>
    <material name="MI_LOGO_03_BK" rgba="1 1 1 1" reflectance="0.5" metallic="0.0" roughness="0.0">
      <layer texture="LOGO_03_BK_Bake1_CyclesBake_COMBINED" role="rgb"/>
    </material>

    <!-- Visual meshes -->
    <mesh class="visual" name="V_ARRO" file="V_ARRO.obj"/>
    <mesh class="visual" name="V_BAKE_INSIDE" file="V_BAKE_INSIDE.obj"/>
    <mesh class="visual" name="V_BAKE_OUTSIDE_A" file="V_BAKE_OUTSIDE_A.obj"/>
    <mesh class="visual" name="V_BAKE_OUTSIDE_B" file="V_BAKE_OUTSIDE_B.obj"/>
    <mesh class="visual" name="V_ENDPOINT" file="V_ENDPOINT.obj"/>
    <mesh class="visual" name="V_ENDPOINT_LIGHT" file="V_ENDPOINT_LIGHT.obj"/>
    <mesh class="visual" name="V_END_light" file="V_END_light.obj"/>
    <mesh class="visual" name="V_LANTERN_BODY_BK" file="V_LANTERN_BODY_BK.obj"/>
    <mesh class="visual" name="V_LANTERN_CLOUR_BK" file="V_LANTERN_CLOUR_BK.obj"/>
    <mesh class="visual" name="V_LOGO_03_BK" file="V_LOGO_03_BK.obj"/>
    <mesh class="visual" name="V_LOGO_03_light" file="V_LOGO_03_light.obj"/>

    <!-- Collision meshes -->
  </asset>

  <default>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2" density="0"/>
      </default>
      <default class="collision">
        <geom type="mesh" rgba="1 1 1 1" group="3" contype="1" conaffinity="1"/>
