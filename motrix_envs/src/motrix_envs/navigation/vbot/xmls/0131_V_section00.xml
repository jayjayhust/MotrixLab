    <!-- Materials -->
    <material name="MI_ARRO_WHITE"
             emission="1.0 1.0 1.0 1.0"
             emissionintensity="10.0"/>
    <material name="MI_ground1_BK" rgba="1 1 1 1" reflectance="0.5" metallic="0.0" roughness="1.0">
      <layer texture="BAKE_INSIDE_Bake1_CyclesBake_COMBINED" role="rgb"/>
      <layer texture="BAKE_INSIDE_Bake1_CyclesBake_COMBINED_ro" role="orm"/>
    </material>
    <material name="MI_BAKE_OUTSIDE_A" rgba="1 1 1 1" reflectance="0.5" metallic="0.0" roughness="0.5">
      <layer texture="BAKE_OUTSIDE_A_Bake1_CyclesBake_COMBINED" role="rgb"/>
